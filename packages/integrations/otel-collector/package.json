{
  "name": "@claude-zen/otel-collector",
  "version": "1.0.0",
  "description": "Internal OpenTelemetry collector for centralized observability data processing and export",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./exporters": {
      "types": "./dist/exporters/index.d.ts",
      "import": "./dist/exporters/index.js"
    },
    "./processors": {
      "types": "./dist/processors/index.d.ts",
      "import": "./dist/processors/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@claude-zen/foundation": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-jaeger": "^2.0.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.203.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.203.0",
    "@opentelemetry/exporter-prometheus": "^0.203.0",
    "@opentelemetry/instrumentation": "^0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "22.17.1",
    "eslint": "^9.33.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "keywords": [
    "opentelemetry",
    "otel",
    "collector",
    "observability",
    "telemetry",
    "metrics",
    "tracing",
    "logging",
    "monitoring",
    "claude-zen"
  ],
  "author": "Claude Code Zen Team",
  "license": "MIT",
  "private": true
}
