{
  "name": "@claude-zen/code-analyzer",
  "version": "1.0.0",
  "type": "module",
  "description": "Live code analysis implementation with AI-powered insights and repository analysis",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@claude-zen/foundation": "workspace:*",
    "@claude-zen/repo-analyzer": "workspace:*",
    "chokidar": "^3.6.0",
    "typescript": "^5.6.3",
    "ts-morph": "^24.0.0",
    "fast-glob": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^22.7.9",
    "eslint": "^9.33.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@eslint/js": "^9.33.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "code-analysis",
    "live-analysis",
    "ai-insights",
    "typescript",
    "javascript",
    "file-watching",
    "complexity-analysis"
  ],
  "author": "Claude Code Zen Team",
  "license": "MIT"
}