{
  "name": "@claude-zen/agent-manager",
  "version": "1.0.1",
  "description": "Ephemeral swarm orchestrator for claude-code-zen with CLI and MCP server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agent-manager": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    },
    "./mcp-server": {
      "types": "./dist/mcp-server.d.ts",
      "import": "./dist/mcp-server.js"
    },
    "./wasm-loader": {
      "types": "./dist/wasm-loader.d.ts",
      "import": "./dist/wasm-loader.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "eslint . --ext .ts --fix",
    "clean": "rimraf dist",
    "start:mcp": "node dist/mcp-server.js"
  },
  "keywords": [
    "agent",
    "swarm",
    "coordination",
    "claude",
    "ai",
    "lifecycle",
    "mcp",
    "cli",
    "ruvswarm",
    "ephemeral",
    "cognitive-diversity"
  ],
  "author": "Claude Code Zen Team",
  "license": "MIT",
  "dependencies": {
    "@claude-zen/foundation": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.17.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@types/node": "22.17.1",
    "eslint": "^9.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "private": true,
  "publishConfig": {
    "access": "restricted"
  }
}
