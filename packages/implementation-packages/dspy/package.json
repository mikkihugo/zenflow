{
  "name": "@claude-zen/dspy",
  "version": "1.0.0",
  "description": "INTERNAL: DSPy Stanford integration engine - Private implementation detail of @claude-zen/brain",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "_prepublishOnly_disabled": "pnpm run clean && pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "docs:check": "node scripts/check-tsdoc.mjs",
    "docs:check-verbose": "node scripts/check-tsdoc.mjs --verbose --show-documented",
    "docs:check-strict": "node scripts/check-tsdoc.mjs --threshold 100",
    "docs:fix": "node scripts/fix-tsdoc.mjs",
    "docs:fix-verbose": "node scripts/fix-tsdoc.mjs --verbose",
    "docs:fix-opus": "node scripts/fix-tsdoc.mjs --model opus"
  },
  "keywords": [
    "dspy",
    "llm",
    "ai",
    "machine-learning",
    "optimization",
    "claude-code-zen"
  ],
  "author": "Claude Code Zen Team",
  "license": "MIT",
  "dependencies": {
    "@claude-zen/neural-ml": "link:../neural-ml",
    "seedrandom": "^3.0.5",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/node": "22.17.1",
    "@types/seedrandom": "^3.0.8",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@claude-zen/foundation": "workspace:*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./brain": {
      "types": "./dist/src/core/brain-service.d.ts",
      "import": "./dist/src/core/brain-service.js"
    },
    "./core": {
      "types": "./dist/src/core/dspy-engine.d.ts",
      "import": "./dist/src/core/dspy-engine.js"
    },
    "./engine": {
      "types": "./dist/src/core/dspy-engine.d.ts",
      "import": "./dist/src/core/dspy-engine.js"
    },
    "./service": {
      "types": "./dist/src/core/service.d.ts",
      "import": "./dist/src/core/service.js"
    },
    "./types": {
      "types": "./dist/src/types/interfaces.d.ts",
      "import": "./dist/src/types/interfaces.js"
    },
    "./core-types": {
      "types": "./dist/src/types/core-types.d.ts",
      "import": "./dist/src/types/core-types.js"
    },
    "./lib": {
      "types": "./dist/src/lib/index.d.ts",
      "import": "./dist/src/lib/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zen-neural/claude-code-zen.git",
    "directory": "packages/dspy"
  }
}
