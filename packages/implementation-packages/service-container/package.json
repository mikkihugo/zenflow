  "author": "Claude Code Zen Team",
  "license": "MIT",
  "dependencies": {
    "@claude-zen/foundation": "workspace:*",
    "awilix": "^11.0.0",
    "neverthrow": "^8.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2",
    "eslint": "^9.17.0"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
