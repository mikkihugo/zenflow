{
  "name": "@claude-zen/service-container",
  "version": "2.1.0",
  "description": "Service Container Implementation Package - Professional DI with Awilix",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "dependency-injection",
    "service-container",
    "awilix",
    "di",
    "ioc",
    "implementation-package"
