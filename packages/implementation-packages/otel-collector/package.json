{
  "name": "@claude-zen/otel-collector",
  "version": "1.0.0",
  "description": "Internal OpenTelemetry collector for centralized observability data processing and export",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./exporters": {
      "types": "./dist/exporters/index.d.ts",
      "import": "./dist/exporters/index.js"
    },
    "./processors": {
      "types": "./dist/processors/index.d.ts", 
      "import": "./dist/processors/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@claude-zen/foundation": "workspace:*",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-node": "^0.45.0",
    "@opentelemetry/exporter-otlp-grpc": "^0.45.0",
    "@opentelemetry/exporter-otlp-http": "^0.45.0",
    "@opentelemetry/exporter-jaeger": "^1.17.0",
    "@opentelemetry/exporter-prometheus": "^0.45.0",
    "@opentelemetry/resources": "^1.17.0",
    "@opentelemetry/semantic-conventions": "^1.17.0",
    "@opentelemetry/instrumentation": "^0.45.0",
    "prom-client": "^15.1.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/node": "^22.17.2",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5",
    "eslint": "^9.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "opentelemetry",
    "otel",
    "collector",
    "observability", 
    "telemetry",
    "metrics",
    "tracing",
    "logging",
    "monitoring",
    "claude-zen"
  ],
  "author": "Claude Code Zen Team",
  "license": "MIT"
}