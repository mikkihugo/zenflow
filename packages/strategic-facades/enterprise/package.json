{
  "name": "@claude-zen/enterprise",
  "version": "1.0.0",
  "description": "Strategic Enterprise Interface Delegation - Business process coordination systems",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ci": "vitest run",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "claude-zen",
    "enterprise",
    "business-processes",
    "safe-framework",
    "sparc",
    "teamwork",
    "agui",
    "knowledge",
    "kanban",
    "strategic-interface",
    "delegation"
  ],
  "author": "Claude Code Zen Team",
  "license": "MIT",
  "dependencies": {
    "@claude-zen/foundation": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@claude-zen/safe-framework": "workspace:*",
    "@claude-zen/sparc": "workspace:*",
    "@claude-zen/teamwork": "workspace:*",
    "@claude-zen/agui": "workspace:*",
    "@claude-zen/knowledge": "workspace:*",
    "@claude-zen/kanban": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@claude-zen/safe-framework": {
      "optional": false
    },
    "@claude-zen/sparc": {
      "optional": false
    },
    "@claude-zen/teamwork": {
      "optional": true
    },
    "@claude-zen/agui": {
      "optional": true
    },
    "@claude-zen/knowledge": {
      "optional": true
    },
    "@claude-zen/kanban": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}