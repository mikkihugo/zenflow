{
  "name": "@claude-zen/dspy",
  "version": "1.0.0",
  "description": "Standalone DSPy integration engine for claude-code-zen",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "dspy",
    "llm",
    "ai",
    "machine-learning",
    "optimization",
    "claude-code-zen"
  ],
  "author": "Claude Code Zen Team",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.4",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "@types/node": "^20.10.6",
    "@types/seedrandom": "^3.0.8"
  },
  "peerDependencies": {
    "@claude-zen/foundation": "workspace:*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./engine": {
      "import": "./dist/engine.js",
      "types": "./dist/engine.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zen-neural/claude-code-zen.git",
    "directory": "src/lib/dspy"
  },
  "publishConfig": {
    "access": "public"
  }
}