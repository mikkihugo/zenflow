{
  "name": "@claude-zen/llm-providers",
  "version": "1.0.0",
  "description": "LLM Provider Integrations - CLI tools, Direct APIs, and AI service integrations",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./claude": {
      "types": "./dist/claude/index.d.ts",
      "import": "./dist/claude/index.js",
      "require": "./dist/claude/index.cjs"
    },
    "./gemini": {
      "types": "./dist/gemini/index.d.ts",
      "import": "./dist/gemini/index.js",
      "require": "./dist/gemini/index.cjs"
    },
    "./cursor": {
      "types": "./dist/cursor/index.d.ts",
      "import": "./dist/cursor/index.js",
      "require": "./dist/cursor/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:ci": "vitest run",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "claude-zen",
    "llm",
    "ai",
    "claude-code",
    "gemini",
    "cursor",
    "cli-providers",
    "artificial-intelligence"
  ],
  "author": "Claude Code Zen Team",
  "license": "MIT",
  "dependencies": {
    "@claude-zen/foundation": "workspace:*",
    "@claude-zen/infrastructure": "workspace:*",
    "@anthropic-ai/claude-code": "^1.0.86",
    "eventemitter3": "^5.0.1",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@types/node": "22.17.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@claude-zen/foundation": "workspace:*"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}