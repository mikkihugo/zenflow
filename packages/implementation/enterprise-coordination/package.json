{
	"name": "@claude-zen/enterprise-coordination",
	"version": "1.0.0",
	"description": "Enterprise coordination implementation - SAFE, SPARC, and project management systems",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"clean": "rm -rf dist",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@claude-zen/foundation": "workspace:*"
	},
	"devDependencies": {
		"@types/node": "^20.19.11",
		"typescript": "^5.9.2"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./coordination": {
			"types": "./dist/coordination/index.d.ts",
			"default": "./dist/coordination/index.js"
		}
	},
	"keywords": [
		"enterprise",
		"coordination",
		"safe",
		"sparc",
		"project-management",
		"agile-release-train",
		"lean-portfolio-management"
