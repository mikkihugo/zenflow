{
  "name": "@claude-zen/teamwork",
  "version": "1.0.0",
  "description": "Universal multi-agent teamwork framework for collaborative problem-solving and sequential decision-making",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "_prepublishOnly_disabled": "pnpm run clean && pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "docs:check": "node scripts/check-tsdoc.mjs",
    "docs:check-verbose": "node scripts/check-tsdoc.mjs --verbose --show-documented",
    "docs:check-strict": "node scripts/check-tsdoc.mjs --threshold 100",
    "docs:fix": "node scripts/fix-tsdoc.mjs",
    "docs:fix-verbose": "node scripts/fix-tsdoc.mjs --verbose",
    "docs:fix-opus": "node scripts/fix-tsdoc.mjs --model opus"
  },
  "keywords": [
    "teamwork",
    "multi-agent",
    "collaborative-problem-solving",
    "sequential-thinking",
    "agent-coordination",
    "cross-agent-communication",
    "collaborative-decision-making",
    "orchestration",
    "memory-management",
    "ai",
    "session-management",
    "universal-agents"
  ],
  "author": "Claude Code Zen Team",
  "license": "MIT",
  "dependencies": {
    "@claude-zen/brain": "file:../brain",
    "@claude-zen/foundation": "file:../foundation",
    "nanoid": "^5.1.5"
  },
  "devDependencies": {
    "@types/node": "^22.17.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./orchestrator": {
      "types": "./dist/orchestrator.d.ts",
      "import": "./dist/orchestrator.js"
    },
    "./storage": {
      "types": "./dist/storage.d.ts",
      "import": "./dist/storage.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zen-neural/claude-code-zen.git",
    "directory": "packages/conversation"
  },
  "publishConfig": {
    "access": "public"
  }
}
