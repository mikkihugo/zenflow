{
  "model_type": "TemporalConvolutionalNetwork",
  "version": "1.0.0",
  "architecture": {
    "name": "OptimizedTCN-PatternDetector",
    "input_dimension": 128,
    "sequence_length": 512,
    "output_dimension": 32,
    "num_channels": [64, 128, 256, 512, 256, 128, 64],
    "kernel_size": 3,
    "dropout": 0.1,
    "activation": "relu",
    "normalization": "batch_norm"
  },
  "layers": [
    {
      "type": "input",
      "shape": [null, 512, 128],
      "name": "code_sequence_input"
    },
    {
      "type": "temporal_conv_block",
      "filters": 64,
      "kernel_size": 3,
      "dilation": 1,
      "dropout": 0.1,
      "residual": true,
      "name": "tcn_block_1"
    },
    {
      "type": "temporal_conv_block",
      "filters": 128,
      "kernel_size": 3,
      "dilation": 2,
      "dropout": 0.1,
      "residual": true,
      "name": "tcn_block_2"
    },
    {
      "type": "temporal_conv_block",
      "filters": 256,
      "kernel_size": 3,
      "dilation": 4,
      "dropout": 0.1,
      "residual": true,
      "name": "tcn_block_3"
    },
    {
      "type": "temporal_conv_block",
      "filters": 512,
      "kernel_size": 3,
      "dilation": 8,
      "dropout": 0.1,
      "residual": true,
      "name": "tcn_block_4"
    },
    {
      "type": "temporal_conv_block",
      "filters": 256,
      "kernel_size": 3,
      "dilation": 16,
      "dropout": 0.1,
      "residual": true,
      "name": "tcn_block_5"
    },
    {
      "type": "temporal_conv_block",
      "filters": 128,
      "kernel_size": 3,
      "dilation": 32,
      "dropout": 0.1,
      "residual": true,
      "name": "tcn_block_6"
    },
    {
      "type": "temporal_conv_block",
      "filters": 64,
      "kernel_size": 3,
      "dilation": 64,
      "dropout": 0.1,
      "residual": true,
      "name": "tcn_block_7"
    },
    {
      "type": "global_average_pooling",
      "name": "global_pooling"
    },
    {
      "type": "dense",
      "units": 128,
      "activation": "relu",
      "dropout": 0.2,
      "name": "pattern_embedding"
    },
    {
      "type": "multi_head_output",
      "heads": {
        "pattern_classification": {
          "units": 16,
          "activation": "softmax",
          "classes": [
            "factory_pattern",
            "singleton",
            "observer",
            "strategy",
            "command",
            "decorator",
            "adapter",
            "facade",
            "template_method",
            "builder",
            "prototype",
            "bridge",
            "composite",
            "flyweight",
            "proxy",
            "chain_of_responsibility"
          ]
        },
        "anti_pattern_detection": {
          "units": 8,
          "activation": "sigmoid",
          "classes": [
            "god_object",
            "spaghetti_code",
            "copy_paste",
            "dead_code",
            "long_method",
            "feature_envy",
            "data_clumps",
            "shotgun_surgery"
          ]
        },
        "refactoring_opportunity": {
          "units": 8,
          "activation": "sigmoid",
          "classes": [
            "extract_method",
            "extract_class",
            "move_method",
            "rename_variable",
            "replace_magic_number",
            "simplify_conditional",
            "remove_duplication",
            "optimize_loop"
          ]
        }
      },
      "name": "multi_output"
    }
  ],
  "optimization": {
    "receptive_field": 4096,
    "parameter_count": 1847552,
    "memory_usage_mb": 7.2,
    "inference_time_ms": 12.3,
    "training_optimizations": {
      "gradient_clipping": 1.0,
      "learning_rate_schedule": "cosine_annealing",
      "weight_decay": 1e-4,
      "early_stopping": {
        "patience": 10,
        "monitor": "val_loss"
      }
    }
  },
  "pattern_detection_features": {
    "code_structure_analysis": {
      "ast_node_types": true,
      "indentation_patterns": true,
      "naming_conventions": true,
      "method_signatures": true
    },
    "temporal_dependencies": {
      "call_sequences": true,
      "variable_lifecycle": true,
      "dependency_chains": true,
      "execution_flow": true
    },
    "swarm_coordination": {
      "agent_interactions": true,
      "task_dependencies": true,
      "communication_patterns": true,
      "collaboration_metrics": true
    }
  },
  "preprocessing": {
    "tokenization": "subword_bpe",
    "vocabulary_size": 8192,
    "sequence_padding": "post",
    "normalization": "layer_norm",
    "feature_extraction": {
      "syntax_features": true,
      "semantic_features": true,
      "structural_features": true,
      "context_features": true
    }
  },
  "postprocessing": {
    "confidence_threshold": 0.75,
    "nms_threshold": 0.5,
    "pattern_clustering": true,
    "temporal_smoothing": true
  }
}
