    "lint": "echo 'ðŸ¤– AI IMPROVEMENT SYSTEM: Standard lint with AI enhancement directives...' && echo '\\nðŸ” Checking for circular dependencies...' && pnpm run lint:circular && echo '\\nðŸ“Š Running TypeScript compilation check...' && tsc --noEmit --skipLibCheck && echo '\\nâš ï¸  AI DIRECTIVE: Fix ALL warnings by EXPANDING code, not disabling rules\\n\\nðŸš€ IMPROVEMENT STRATEGIES:\\n1. Replace any types with specific interfaces\\n2. Add import type for type-only imports\\n3. Fix floating promises with proper error handling\\n4. Add exhaustive switch cases\\n5. Preserve unused code with comments explaining why\\n6. Optimize performance with better data structures\\n\\nâ­ TARGET: Zero warnings through intelligent code enhancement' && pnpm run lint:rust && (tsc --noEmit --skipLibCheck > /dev/null 2>&1 && echo '\\nâœ… Code is clean, running TSDoc analysis...' && pnpm run docs:check || echo '\\nâš ï¸  TSDoc skipped - fix TypeScript errors first')",
