{
  "name": "@zen-ai/claude-code-zen",
  "version": "1.0.1",
  "type": "module",
  "description": "Claude Code Zen: 5-Tier strategic architecture with AI swarm orchestration, neural networks, and web dashboard. Features strategic facade delegation, internal implementation packages, and deep internal dependencies.",
  "main": "bin/claude-zen.cjs",
  "bin": {
    "claude-zen": "bin/claude-zen.cjs"
  },
  "preferGlobal": true,
  "workspaces": [
    "apps/claude-code-zen-server",
    "apps/web-dashboard",
    "packages/public-api/core/*",
    "packages/public-api/facades/*",
    "packages/implementation/*",
    "packages/enterprise/*",
    "packages/private-core/*"
  ],
  "scripts": {
    "start": "pnpm --filter @claude-zen/server start",
    "dev": "concurrently \"pnpm --filter @claude-zen/server dev\" \"pnpm --filter @claude-zen/web-dashboard dev\"",
    "build": "node scripts/build.js",
    "build:packages": "pnpm -r build --if-present",
    "build:rust": "node scripts/build-rust-simple.js",
    "build:rust-full": "node scripts/build-rust.js",
    "build:all": "pnpm run build:packages && pnpm run build:rust && pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:all": "pnpm -r test --if-present",
    "lint": "NODE_OPTIONS=\"--max-old-space-size=8192\" eslint . --fix && prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" && find . -name '*.rs' -not -path '*/target/*' -not -path '*/node_modules/*' | head -20 | xargs -r rustfmt --edition=2024",
    "lint:packages": "pnpm -r lint --if-present",
    "type-check": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsc --noEmit --skipLibCheck",
    "type-check:packages": "pnpm -r type-check --if-present",
    "clean": "rm -rf dist/ node_modules/.cache/ && pnpm run clean:all",
    "clean:all": "pnpm -r clean --if-present && find . -name 'dist' -type d -not -path '*/node_modules/*' -not -path './dist' -exec rm -rf {} + 2>/dev/null || true && find . -name 'target' -type d -not -path '*/node_modules/*' -exec rm -rf {} + 2>/dev/null || true",
    "clean:rust": "find . -name 'target' -type d -not -path '*/node_modules/*' -exec rm -rf {} + 2>/dev/null || true",
    "clean:typescript": "find . -name 'dist' -type d -not -path '*/node_modules/*' -not -path './dist' -exec rm -rf {} + 2>/dev/null || true",
    "validate:imports": "node scripts/validate-imports.js",
    "validate:dependencies": "node scripts/validate-dependencies.js",
    "validate:foundation": "pnpm run validate:imports && pnpm run validate:dependencies",
    "prepare": "husky install || true",
    "pre-commit": "bash scripts/pre-commit.sh"
  },
  "files": [
    "dist/",
    "bin/",
    "package.json",
    "docs/",
    "scripts/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "ai",
    "swarm",
    "orchestration",
    "neural-networks",
    "coordination",
    "3-tier-architecture",
    "strategic-facades",
    "implementation-packages",
    "deep-internal-dependencies",
    "wasm",
    "web-dashboard",
    "typescript",
    "web-interface"
  ],
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.90",
    "@anthropic-ai/sdk": "^0.60.0",
    "@floating-ui/dom": "^1.7.4",
    "@godaddy/terminus": "^4.12.1",
    "@lancedb/lancedb": "^0.21.3",
    "@logtape/file": "^1.0.4",
    "@logtape/logtape": "^1.0.4",
    "@octokit/rest": "^22.0.0",
    "@sveltejs/adapter-node": "^5.3.1",
    "@sveltejs/kit": "^2.36.2",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/better-sqlite3": "^7.6.13",
    "autoprefixer": "^10.4.21",
    "awilix": "^12.0.5",
    "better-sqlite3": "^12.2.0",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "compression": "^1.8.1",
    "connect-redis": "^9.0.0",
    "consistent-hashing": "^0.3.0",
    "cors": "^2.8.5",
    "eventemitter3": "^5.0.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "express-slow-down": "^3.0.0",
    "express-status-monitor": "^1.3.4",
    "express-validator": "^7.2.1",
    "express-ws": "^5.0.2",
    "fs-extra": "^11.3.1",
    "hashring": "^3.2.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.7.0",
    "js-yaml": "^4.1.0",
    "keyv": "^5.5.0",
    "kuzu": "^0.11.2",
    "level": "^10.0.0",
    "listr2": "^9.0.2",
    "lucide-svelte": "^0.541.0",
    "markdown-it": "^14.1.0",
    "mermaid": "^11.10.1",
    "morgan": "^1.10.1",
    "nanoid": "^5.1.5",
    "node-cron": "^4.2.1",
    "node-os-utils": "^1.3.7",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "prismjs": "^1.30.0",
    "prom-client": "^15.1.3",
    "redis": "^5.8.2",
    "seedrandom": "^3.0.5",
    "semver": "^7.7.2",
    "simple-statistics": "^7.8.8",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "svelte": "^5.38.3",
    "svelte-check": "^4.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwindcss": "^4.1.12",
    "ts-morph": "^26.0.0",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "ws": "^8.18.3",
    "yaml": "^2.8.1",
    "zod": "^4.1.1",
    "complexity-report": "^2.0.0-alpha"
  },
  "devDependencies": {
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "eslint-formatter-unix": "^8.40.0",
    "@eslint/js": "^9.34.0",
    "@types/blessed": "^0.1.25",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.18.0",
    "@types/node-cron": "^3.0.11",
    "@types/on-finished": "^2.3.5",
    "@types/response-time": "^2.3.9",
    "@types/seedrandom": "^3.0.8",
    "@types/semver": "^7.7.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vercel/ncc": "^0.38.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@yao-pkg/pkg": "^6.6.0",
    "concurrently": "^9.2.1",
    "dependency-cruiser": "^17.0.1",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "eslint-formatter-compact": "^8.40.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "execa": "^9.6.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5"
  },
  "engines": {
    "node": ">=22.0.0 <24.0.0",
    "pnpm": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/claude-code-zen.git"
  },
  "author": "Claude Code Zen Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-org/claude-code-zen/issues"
  },
  "homepage": "https://github.com/your-org/claude-code-zen#readme"
}
