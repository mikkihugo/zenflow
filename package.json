{
  "name": "claude-code-zen",
  "version": "2.0.0-alpha.73",
  "description": "Claude Code Zen: Unified AI swarm orchestration platform with neural networks and direct integration",
  "main": "dist/claude-zen-core.js",
  "bin": {
    "claude-zen": "./bin/claude-zen"
  },
  "scripts": {
    "start": "npx tsx src/claude-zen-core.ts",
    "dev": "npx tsx src/claude-zen-integrated.ts --port 3000",
    "dev:tui": "npx tsx src/interfaces/terminal/InteractiveTerminalApplication.tsx",
    "dev:web": "CLAUDE_ZEN_DAEMON=true npx tsx src/interfaces/web/web-interface.ts --daemon",
    "build": "npm run clean && npm run update-version && tsc",
    "build:wasm": "cd src/neural/wasm && ./scripts/build-wasm.sh",
    "build:wasm:dev": "cd src/neural/wasm && BUILD_MODE=dev ./scripts/build-wasm.sh",
    "build:wasm:prod": "cd src/neural/wasm && OPTIMIZE_SIZE=true BUILD_MODE=release ./scripts/build-wasm.sh",
    "clean": "rm -rf dist",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --maxWorkers=1 --forceExit --testTimeout=30000",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest src/__tests__/unit",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules' jest src/__tests__/integration",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules' jest src/__tests__/e2e",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage --collectCoverageFrom='src/**/*.{ts,js}' --coverageReporters=text,lcov,html",
    "test:comprehensive": "node scripts/run-infrastructure-tests.js && npm run test:coverage",
    "test:ci": "NODE_OPTIONS='--experimental-vm-modules' jest --ci --coverage --maxWorkers=2",
    "test:debug": "NODE_OPTIONS='--experimental-vm-modules --inspect-brk' jest --runInBand --no-cache",
    "test:neural": "node src/bindings/test/test.js",
    "test:tdd-london": "node scripts/run-tdd-london-tests.js",
    "lint": "npm run lint:biome && eslint . --cache --fix",
    "lint:biome": "biome check --write .",
    "format": "npm run format:biome && prettier --write src",
    "format:biome": "biome format --write .",
    "mcp": "npx tsx src/interfaces/mcp/start-server.ts",
    "mcp:swarm": "npx tsx src/coordination/mcp/claude-zen-server.ts",
    "mcp:test": "npx tsx src/interfaces/mcp/test-external-servers.ts",
    "mcp:external": "npx tsx src/interfaces/mcp/test-external-servers.ts",
    "mcp:validate": "npx tsx src/interfaces/mcp/validate-external-config.ts",
    "server": "npx tsx src/claude-zen-integrated.ts",
    "benchmark": "node src/coordination/diagnostics/benchmark.js",
    "docs:build": "node scripts/generate-docs.js",
    "docs:enhanced": "node scripts/enhanced-docs-generator.js",
    "docs:full": "npm run docs:build && npm run docs:enhanced",
    "security:audit": "npm audit --audit-level=moderate",
    "security:harden": "node scripts/security-hardening.js",
    "security:full": "npm run security:audit && npm run security:harden",
    "diagnostics": "node -e \"import('./dist/monitoring/diagnostics.js').then(m => m.DiagnosticManager.quickDiagnostic().then(console.log))\"",
    "health-check": "node -e \"import('./dist/monitoring/health-check.js').then(m => new m.HealthCheckManager().performHealthCheck().then(console.log))\"",
    "update-version": "npx tsx scripts/update-bin-version.js",
    "postinstall": "npx tsx scripts/install.ts",
    "prepublishOnly": "npm run update-version",
    "preflight": "npm run build && npm run test && npm run lint",
    "publish:alpha": "npm publish --tag alpha",
    "publish:patch": "npm version patch && npm publish",
    "sync:check": "node scripts/sync-check.js",
    "deps:check": "npx depcruise src",
    "deps:graph": "npx depcruise --output-to dependency-graph.svg src",
    "deps:html": "npx depcruise --output-to dependency-report.html --output-type html src",
    "deps:circular": "npx depcruise --output-type err-only --config .dependency-cruiser.cjs src",
    "build:fast": "npm run clean && tsc --incremental --skipLibCheck",
    "build:core": "tsc src/core/*.ts src/interfaces/*.ts --outDir dist/core --module ESNext --target ES2022",
    "build:check": "tsc --noEmit --skipLibCheck",
    "test:quick": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --passWithNoTests --bail",
    "prestart": "npm run build:check",
    "start:dev": "npx tsx src/claude-zen-integrated.ts --dev"
  },
  "keywords": [
    "claude",
    "ai",
    "agent",
    "orchestration",
    "mcp",
    "workflow",
    "automation",
    "swarm",
    "ruv-swarm",
    "github",
    "docker",
    "enterprise",
    "coordination",
    "multi-agent",
    "neural-networks",
    "cli",
    "tools",
    "alpha"
  ],
  "author": "rUv",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikkihugo/claude-code-zen.git"
  },
  "bugs": {
    "url": "https://github.com/mikkihugo/claude-code-zen/issues"
  },
  "homepage": "https://github.com/mikkihugo/claude-code-zen#readme",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "files": [
    "dist/",
    "src/",
    ".claude/",
    "scripts/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@ag-ui/core": "^0.0.35",
    "@google/generative-ai": "^0.24.1",
    "@lancedb/lancedb": "^0.21.1",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/socket.io": "^3.0.1",
    "@typescript-eslint/parser": "^8.38.0",
    "acorn": "^8.10.0",
    "better-sqlite3": "^12.2.0",
    "chalk": "^5.4.1",
    "escomplex": "^1.3.0",
    "esprima": "^4.0.1",
    "express": "^5.1.0",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.3",
    "helmet": "^8.1.0",
    "ink": "^6.0.1",
    "ink-select-input": "^6.2.0",
    "inquirer": "^12.8.2",
    "jscpd": "^3.5.10",
    "kuzu": "^0.11.1",
    "markdownlint": "^0.38.0",
    "meow": "^13.2.0",
    "nanoid": "^5.1.5",
    "ora": "^8.2.0",
    "react": "^19.1.0",
    "read-pkg-up": "^10.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "tree-sitter": "^0.25.0",
    "tree-sitter-javascript": "^0.20.4",
    "tree-sitter-typescript": "^0.20.5",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@biomejs/biome": "^2.1.3",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.31.0",
    "@playwright/test": "^1.54.1",
    "@types/blessed": "^0.1.25",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/gradient-string": "^1.1.6",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "axios": "^1.10.0",
    "babel-jest": "^30.0.5",
    "dependency-cruiser": "^17.0.0",
    "eslint": "^9.31.0",
    "eslint-formatter-compact": "^8.40.0",
    "eslint-formatter-unix": "^8.40.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "jest-extended": "^6.0.0",
    "js-yaml": "^4.1.0",
    "k6": "^0.0.0",
    "playwright": "^1.54.1",
    "prettier": "^3.6.2",
    "puppeteer": "^24.14.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
