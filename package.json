{
  "name": "@zen-ai/claude-code-zen",
  "version": "1.0.1",
  "type": "module",
  "description": "Claude Code Zen: 5-Tier strategic architecture with AI swarm orchestration, neural networks, and web dashboard. Features strategic facade delegation, internal implementation packages, and deep internal dependencies.",
  "main": "dist/claude-zen.js",
  "bin": {
    "claude-zen": "dist/claude-zen.js"
  },
  "preferGlobal": true,
  "private": true,
  "workspaces": [
    "apps/claude-code-zen-server",
    "apps/web-dashboard",
    "packages/public-api/core/*",
    "packages/public-api/facades/*"
  ],
  "scripts": {
    "start": "pnpm --filter @claude-zen/server start",
    "dev": "concurrently \"pnpm --filter @claude-zen/server dev\" \"pnpm --filter @claude-zen/web-dashboard dev\"",
    "build": "echo 'Building foundation...' && cd packages/public-api/core/foundation && pnpm build && cd - && echo 'Building server...' && cd apps/claude-code-zen-server && pnpm build && cd - && echo 'Building web dashboard...' && cd apps/web-dashboard && pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "NODE_OPTIONS=\"--max-old-space-size=8192\" eslint . --fix && prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" && find . -name '*.rs' -not -path '*/target/*' -not -path '*/node_modules/*' | head -20 | xargs -r rustfmt --edition=2024",
    "type-check": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsc --noEmit --skipLibCheck",
    "clean": "rm -rf dist/ node_modules/.cache/ target/ && pnpm --filter @claude-zen/server clean && pnpm --filter @claude-zen/web-dashboard clean"
  },
  "files": [
    "dist/",
    "bin/",
    "package.json",
    "src/",
    "docs/",
    "scripts/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "ai",
    "swarm",
    "orchestration",
    "neural-networks",
    "coordination",
    "3-tier-architecture",
    "strategic-facades",
    "implementation-packages",
    "deep-internal-dependencies",
    "wasm",
    "web-dashboard",
    "typescript",
    "web-interface"
  ],
  "dependencies": {
