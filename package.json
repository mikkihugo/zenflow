  "homepage": "https://github.com/mikkihugo/claude-code-zen#readme",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "files": [
    "dist/",
    "src/",
    ".claude/",
    "scripts/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
